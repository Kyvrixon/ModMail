{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
	"labels": ["Dep Updates"],
    "packageRules": [
        {
            "schedule": [
                "at any time"
            ],
            "matchPackageNames": [
                "*"
            ]
        }
    ],
    "dependencyDashboard": false,
    "rangeStrategy": "bump"
}